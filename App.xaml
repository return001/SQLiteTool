<Application x:Class="SQLiteTool.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SQLiteTool"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!--区域化-->
                <!--默认简体中文-->
                <ResourceDictionary Source="Resources/Localization/zh-CN.xaml"/>
               
                <ResourceDictionary Source="Style/WindowStyle.xaml"/>
                <ResourceDictionary Source="Style/ImageButton.xaml"/>
                <ResourceDictionary Source="Style/RichTextBoxStyle.xaml"/>


            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
